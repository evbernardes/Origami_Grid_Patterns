<!--			<param name="cut_dashes_bool" type="boolean" _gui-text="Dashed strokes?">false</param>-->
<!--			<param name="cut_dashes_len" type="float"  min="0.1" max="10" appearance="full" precision="2" _gui-text="Cut dash + gap length">1</param>-->
