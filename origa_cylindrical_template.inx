    </param>
    
    <effect>
    <object-type>all</object-type>
        <effects-menu>
            <submenu _name="Origami Patterns">
                <submenu _name="Cylindrical" />
            </submenu>
        </effects-menu>
    </effect>

    <script>
        <command reldir="extensions" interpreter="python">OrigamiPatterns/Cylindrical_Template.py</command>
    </script>

</inkscape-extension>
